%h1 Home#index

%ul
-if user_signed_in?
  %li= link_to "Log Out", destroy_user_session_path, method: :delete
-else
  %li= link_to 'Sign In', new_user_session_path
  %li= link_to 'Sign Up', new_user_registration_path

%ul
  -@products.each do |p|
    %li
      = p.title
      = render 'shopping_cart/carts/add_btn', product: p